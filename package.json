{
  "name": "@taujs/server",
  "version": "0.2.4",
  "description": "taujs | Ï„js",
  "author": "Aoede <taujs@aoede.uk.net> (https://www.aoede.uk.net)",
  "license": "MIT",
  "homepage": "https://github.com/aoede3/taujs-server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoede3/taujs-server.git"
  },
  "bugs": {
    "url": "https://github.com/aoede3/taujs-server/issues"
  },
  "keywords": [
    "fastify",
    "typescript",
    "esm",
    "vite",
    "streaming",
    "react",
    "ssr"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./build": {
      "import": "./dist/build.js",
      "types": "./dist/build.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fastify/static": "^8.0.3",
    "path-to-regexp": "^8.1.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@babel/preset-typescript": "^7.24.7",
    "@changesets/cli": "^2.27.7",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^2.1.0",
    "fastify": "^5.3.3",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^6.3.5",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "fastify": "^5.3.3",
    "typescript": "^5.5.4",
    "vite": "^6.3.5"
  },
  "scripts": {
    "build": "tsup",
    "build-local": "tsup && ./move.sh",
    "ci": "npm run build && npm run check-format && npm run lint",
    "lint": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "prepublishOnly": "npm run ci",
    "local-release": "npm run ci && changeset version && changeset publish"
  }
}
